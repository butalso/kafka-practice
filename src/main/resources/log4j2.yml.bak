  Console:
   "-name": Console
   PatternLayout:
    "-pattern": %d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n
 Loggers:
  Root:
   "-level": trace
   AppenderRef:
    "-ref": Console
